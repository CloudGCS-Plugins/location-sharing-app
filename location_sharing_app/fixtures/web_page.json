  "main_section_html": "<!DOCTYPE html>\r\n<html lang=\"tr\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <style>\r\n        h1 {\r\n            margin-top: 0;\r\n        }\r\n\r\n        .nav-link, .nav-avatar, .navbar-toggler, .web-footer {\r\n            display: none;\r\n        }\r\n\r\n        .container{\r\n            padding: 0, 16;\r\n            margin: 0;\r\n        }\r\n\r\n        .container-wrapper {\r\n            display: flex;\r\n            width: 100%;\r\n            height: 90vh;\r\n            flex-direction: row;\r\n        }\r\n\r\n        .navbar-brand > span {\r\n           text-decoration: underline;\r\n           cursor: pointer !important;\r\n        }\r\n\r\n        .blue-section {\r\n            background: linear-gradient(135deg, #A0A0C0, oklch(0.962 0.018 272.314));\r\n            height: 100vh;\r\n            width: 50%;\r\n            position: fixed;\r\n            top: 0;\r\n            right: 0;\r\n            max-width: 100%;\r\n            max-height: 100%;\r\n        }\r\n\r\n        .content-section {\r\n            width: 90vh;\r\n            display: flex;\r\n            align-items: center;\r\n            justify-content: center;\r\n            padding: 20px;\r\n            left: 0;\r\n        }\r\n\r\n        .content-box {\r\n            display: flex;\r\n            flex-direction: column;\r\n            align-items: center;\r\n            max-width: 65vh;\r\n            max-height: 100%;\r\n            padding: 0 15px;\r\n        }\r\n\r\n        form{\r\n            width: 100%;\r\n        }\r\n\r\n        .form-group {\r\n            width: 100%;\r\n            margin-bottom: 15px;\r\n        }\r\n\r\n        label {\r\n            font-weight: bold;\r\n            font-size: 16px;\r\n            display: block;\r\n        }\r\n\r\n        input, textarea {\r\n            width: 100%;\r\n            padding: 12px;\r\n            border: 1px solid #ccc;\r\n            border-radius: 8px;\r\n            font-size: 16px;\r\n        }\r\n\r\n        textarea {\r\n            resize: vertical; /* Only allows vertical resize */\r\n            min-height: 100px;\r\n            max-height: 300px; /* Prevents textarea from getting too big */\r\n            overflow-y: auto; /* Adds scrollbar when needed */\r\n        }\r\n\r\n        button {\r\n            width: 100%;\r\n            padding: 14px;\r\n            background: #007BFF;\r\n            color: white;\r\n            border: none;\r\n            border-radius: 8px;\r\n            cursor: pointer;\r\n            font-size: 16px;\r\n        }\r\n\r\n        .link {\r\n            margin-top: 10px;\r\n            text-align: center;\r\n        }\r\n\r\n        .link a {\r\n            color: black;\r\n            text-decoration: underline;\r\n        }\r\n\r\n        @media (max-width: 1024px) {\r\n            .container-wrapper {\r\n                flex-direction: column;\r\n            }\r\n            .blue-section {\r\n                height: 20vh;\r\n                width: 100%;\r\n                position: relative;\r\n            }\r\n            .content-section {\r\n                min-height: 75vh;\r\n                width: 100%;\r\n                position: relative;\r\n            }\r\n        }\r\n\r\n        .contact-box {\r\n            position: absolute;\r\n            top: 30px;\r\n            right: 30px;\r\n            background-color: rgba(255, 255, 255, 0.8);\r\n            padding: 16px;\r\n            margin-top: 30px;\r\n            border-radius: 8px;\r\n            box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.1);\r\n            max-width: 300px;\r\n        }\r\n\r\n        .contact-title {\r\n            font-size: 18px;\r\n            color: #3F51B5;\r\n            margin: 0;\r\n            font-weight: 600;\r\n        }\r\n\r\n        .contact-info {\r\n            font-size: 14px;\r\n            color: #333333;\r\n            margin: 8px 0;\r\n        }\r\n\r\n        body {\r\n            font-family: Arial, sans-serif;\r\n        }\r\n\r\n        @media (max-width: 600px) {\r\n            .contact-box {\r\n                top: 20px;\r\n                right: 0;\r\n            }\r\n        }\r\n    </style>\r\n    <script>\r\n        function sendContactUs() {\r\n            var full_name = document.getElementById(\"full-name\").value;\r\n            var email = document.getElementById(\"email\").value;\r\n            var subject = document.getElementById(\"subject\").value;\r\n            var message = document.getElementById(\"message\").value;\r\n\r\n            if (full_name == \"\" || email == \"\" || subject == \"\" || message == \"\") {\r\n                return;\r\n            }\r\n\r\n            var data = {\r\n                full_name: full_name,\r\n                email: email,\r\n                subject: subject,\r\n                message: message\r\n            };\r\n\r\n\r\n            frappe.call({\r\n                method:\r\n                    \"location_sharing_app.location_sharing_app.doctype.contact_us.contact_us.send_message\",\r\n                args: {\r\n                    full_name: full_name,\r\n                    email: email,\r\n                    subject: subject,\r\n                    message: message\r\n                },\r\n                callback: function (r) {\r\n                    alert(\"Message sent successfully!\");\r\n                },\r\n            });\r\n        }\r\n    </script>\r\n</head>\r\n<body>\r\n    <div class=\"container-wrapper\">\r\n        <div class=\"blue-section\"></div>\r\n        <div class=\"contact-box\">\r\n            <h6 class=\"contact-title\">Contact Information</h6>\r\n            <p class=\"contact-info\">Phone: +353 61 475 895</p>\r\n            <p class=\"contact-info\">E-mail: contact@a-techsyn.com</p>\r\n            <p class=\"contact-info\">Location: Shannon/IRELAND</p>\r\n        </div>\r\n        <div class=\"content-section\">\r\n            <div class=\"content-box\">\r\n                <h3>Contact Us</h3>\r\n                <p>If you fill in the relevant fields and send, we will get back to you regarding the issue. Our response will be to the e-mail address you provided.</p>\r\n                <form>\r\n                    <div class=\"form-group\">\r\n                        <label for=\"full-name\">Name</label>\r\n                        <input type=\"text\" id=\"full-name\" placeholder=\"Name\" required>\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                        <label for=\"email\">E-mail</label>\r\n                        <input type=\"email\" id=\"email\" placeholder=\"example@mail.com\" required>\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                        <label for=\"subject\">Subject</label>\r\n                        <input type=\"text\" id=\"subject\" placeholder=\"Subject\" maxlength=\"45\" required>\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                        <label for=\"message\">Message</label>\r\n                        <textarea id=\"message\" placeholder=\"Enter your message - Max 150 characters -\" maxlength=\"150\" rows=\"5\" required></textarea>\r\n                    </div>\r\n                    <button type=\"submit\" onclick=\"sendContactUs()\">Send</button>\r\n                </form>\r\n                <div class=\"link\" style=\"padding: 25px;\">\r\n                    <a href=\"/help\">Account Delete Steps</a>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</body>\r\n</html>\r\n",
